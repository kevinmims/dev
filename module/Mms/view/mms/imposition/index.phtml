		            this.api().columns().every( function () {
		                var column = this;

		                var select = $('<select class="filter"><option value=""></option></select>')
		                    .appendTo( $(column.footer()).empty() )
		                    .on( 'change', function () {
		                        var val = $.fn.dataTable.util.escapeRegex(
		                            $(this).val()
