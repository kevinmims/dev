		triggerMxmlFileSelect: function($e){
			var input = $e,
	            numFiles = input.get(0).files ? input.get(0).files.length : 1,
	            files = $e.prop("files"),
	            fileNames = $.map(files, function(val) { return val.name; });
	        input.trigger('fileselect', [numFiles, fileNames]);
		},

